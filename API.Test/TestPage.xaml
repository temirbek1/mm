        <Button Text="Update Order" Clicked="OnUpdateOrderClicked" />
        <Button Text="Get Customer By Phone" Clicked="OnGetCustomerByPhoneClicked" />
        <Label x:Name="ResultLabel" />
    </StackLayout>
</ContentPage>
